<?xml version="1.0" encoding="utf-8"?>
<com.vpn.base.VpnToolbar xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageButton
        android:layout_marginTop="21dp"
        android:layout_gravity="left"
        android:id="@+id/navbar_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:background="@mipmap/main_menu" />

    <ImageButton
        android:layout_marginTop="21dp"
        android:layout_gravity="right"
        android:id="@+id/navbar_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:background="@mipmap/main_server" />

</com.vpn.base.VpnToolbar>